{
  "manifest_version": 2,
  "name": "Goto foo",
  "version": "1.6",
  "description": "Adds \"Goto <selected text as url>\" to the right-click menu.",
  "homepage_url": "https://github.com/pmarks-net/gotofoo",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "permissions": ["contextMenus", "<all_urls>"]
}

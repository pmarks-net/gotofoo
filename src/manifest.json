{
  "manifest_version": 2,
  "name": "Goto foo",
  "version": "1.4",
  "description": "Adds a 'Goto <url>' option when right-clicking text that resembles a URL.",
  "homepage_url": "https://github.com/pmarks-net/gotofoo",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "permissions": ["contextMenus", "<all_urls>"]
}

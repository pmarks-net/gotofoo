{
  "manifest_version": 3,
  "name": "Goto foo",
  "version": "1.7",
  "description": "Adds \"Goto <selected text as url>\" to the right-click menu.",
  "homepage_url": "https://github.com/pmarks-net/gotofoo",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "permissions": ["contextMenus"],
  "optional_host_permissions": [
    "<all_urls>"
  ]
}
